snippet cla
	class ${1:ClassName}
		${2:code}
	end

snippet cla<
	class ${1:ClassName} < ${2:ParentClass}
		${3}
	end

snippet ed
	${1}.each do |${2:$1}|
	  ${3}
	end
	$0

snippet ea
	${1}.each { |${2:$1}| ${3} }
	$0

snippet defp
	def ${1:method_name}(${2:params})
	  ${3}
	end

snippet :. "key: key"
	${1:key}: ${2:$1}$0
